<html>
<head>
    <title>
        NG_App
    </title>
</head>
<body>
    <div ng-app="app">
        <div ng-controller="appCtrl as Ctrl1">
            <h1>Title:  {{Ctrl1.pageTitle}}</h1>
        </div>
        <div ng-controller="appCtr">
            <div id="nameDiv" >
                <table>
                    <thead>
                        <tr>
                            <th>
                                Questions
                            </th>
                            <th>
                                Level
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="question in questions">
                            <td>
                                {{question.value}}
                            </td>
                            <td>
                                {{question.difficultLevel}}
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        
    </div>
    <script src="~/lib/angular/angular.js"></script>

    <script src="~/js/App.js"></script>
</body>
</html>